  (select count(*) from reporte_evaluacion_consolidado_et eva where eva.mes_08 is not null and eva.zona = rpt.zona and eva.mes_08::numeric = rpt.id_nivel_evaluacion_detalle and eva.aud_usuario = $P{par_id_usuario} ) +
  (select count(*) from reporte_evaluacion_consolidado_et eva where eva.mes_09 is not null and eva.zona = rpt.zona and eva.mes_09::numeric = rpt.id_nivel_evaluacion_detalle and eva.aud_usuario = $P{par_id_usuario} ) +
