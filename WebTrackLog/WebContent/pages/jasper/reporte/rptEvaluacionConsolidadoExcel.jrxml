  (select count(*) from reporte_evaluacion_consolidado_et eva where eva.mes_04 is not null and eva.zona = rpt.zona and eva.mes_04::numeric = rpt.id_nivel_evaluacion_detalle and eva.aud_usuario = $P{par_id_usuario} ) +
  (select count(*) from reporte_evaluacion_consolidado_et eva where eva.mes_05 is not null and eva.zona = rpt.zona and eva.mes_05::numeric = rpt.id_nivel_evaluacion_detalle and eva.aud_usuario = $P{par_id_usuario}  ) +
  (select count(*) from reporte_evaluacion_consolidado_et eva where eva.mes_06 is not null and eva.zona = rpt.zona and eva.mes_06::numeric = rpt.id_nivel_evaluacion_detalle and eva.aud_usuario = $P{par_id_usuario} ) +
