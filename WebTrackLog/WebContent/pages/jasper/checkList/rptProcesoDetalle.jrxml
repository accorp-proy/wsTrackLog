det.puntaje,
det.esfuerzo,
det.id_proceso_detalle as id,
pro.nombre_proceso as nombreProceso,
det.nombre_proceso_detalle as descripcion
from proceso_detalle_et det
inner join proceso_et pro
on det.id_proceso=pro.id_proceso
