(CASE WHEN kpi.valor_diferencia < 0 THEN 'true' else 'false' end ) as condicionColor
from  check_list_kpi_ejecucion_et kpi
inner join tipo_estacion_et tip
on kpi.id_tipo_estacion = tip.id_tipo_estacion
